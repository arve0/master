@article{brabrand_alterations_2015,
	title = {Alterations in collagen fibre patterns in breast cancer. A premise for tumour invasiveness?},
	volume = {123},
	rights = {© 2014 {APMIS}. Published by John Wiley \& Sons Ltd},
	issn = {1600-0463},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/apm.12298/abstract},
	doi = {10.1111/apm.12298},
	abstract = {Stromal tissue in the breast plays a key role in cancer invasiveness due to molecular and cellular changes. Collagen is the main component of the stroma. The purposes of this study were to investigate differences in collagen fibre patterns between tumour-induced stromal tissue and normal stroma, and between high-grade and low-grade breast cancer stroma, using second harmonic generation microscopy. Thirty-seven ductal carcinomas were examined: Twenty-one Luminal A phenotype and sixteen {HER}2 or Basal-like phenotype. Three regions were examined in each case: intratumoral, juxtatumoral and extratumoral. Two images were captured in each region. Two characteristics of collagen fibres were examined: the degree of straightness, and the degree of alignment. Collagen fibres were visually classified as curly, intermediate or straight, and as parallel or not parallel. The results of angle measurement and visual analysis showed that collagen fibres were straightest in the intratumoral region and curliest in the extratumoral region. Collagen fibres were more parallel in the juxtatumoral region compared to the two other regions. There were no significant differences between high-grade and low-grade tumours. As a breast tumour progresses, collagen fibres appear to straighten and align at the tumour boundary. This could facilitate invasion of the tumour into the surrounding stroma.},
	pages = {1--8},
	number = {1},
	journaltitle = {{APMIS}},
	shortjournal = {{APMIS}},
	author = {Brabrand, Anders and Kariuki, Ian I. and Engstrøm, Monica J. and Haugen, Olav A. and Dyrnes, Linda A. and Åsvold, Bjørn O. and Lilledahl, Magnus B. and Bofin, Anna M.},
	urldate = {2015-05-23},
	date = {2015-01-01},
	langid = {english},
	keywords = {Breast cancer, collagen fibre pattern, second harmonic generation microscopy, tumour invasion},
	file = {Full Text PDF:/Users/arve/Library/Application Support/Zotero/Profiles/h0ttneme.default/zotero/storage/GJ9GSSVV/Brabrand et al. - 2015 - Alterations in collagen fibre patterns in breast c.pdf:application/pdf;Snapshot:/Users/arve/Library/Application Support/Zotero/Profiles/h0ttneme.default/zotero/storage/TTQ8Z3S2/abstract\;jsessionid=10857C126140A825A3FA78273E6B0E31.html:text/html}
}

